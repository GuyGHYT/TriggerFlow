# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to git

# Spotify API credentials (get from https://developer.spotify.com)
SPOTIPY_CLIENT_ID=your_spotify_client_id_here
SPOTIPY_CLIENT_SECRET=your_spotify_client_secret_here  
SPOTIPY_REDIRECT_URI=http://localhost:8888/callback

# Voicemeeter Remote DLL path (optional, auto-detected if installed normally)
# VOICEMEETER_DLL=C:\Program Files (x86)\VB\Voicemeeter\VoicemeeterRemote64.dll

# Voicemod API settings (optional, defaults shown)
# VOICEMOD_PORT=59129
# VOICEMOD_HOST=localhost